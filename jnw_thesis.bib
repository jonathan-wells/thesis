pmid = {26654279},
publisher = {Elsevier Ltd},
title = {{Crosslinking and Mass Spectrometry: An Integrated Technology to Understand the Structure and Function of Molecular Machines}},
url = {http://dx.doi.org/10.1016/j.tibs.2015.10.008},
volume = {41},
year = {2016}
