journal = {Molecular Systems Biology},
keywords = {codon usage,translation accuracy,translation effi-},
number = {481},
pages = {481},
pmid = {21487400},
publisher = {Nature Publishing Group},
title = {{Determinants of translation efficiency and accuracy}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3101949/{\%}5Cnhttp://msb.embopress.org/content/7/1/481.full.pdf{\%}5Cnhttp://msb.embopress.org/content/7/1/481.long{\%}5Cnhttp://www.nature.com/msb/journal/v7/n1/full/msb201114.html{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed},
