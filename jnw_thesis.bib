number = {2},
pages = {150005},
pmid = {25716199},
title = {{Three-dimensional topology of the SMC2/SMC4 subcomplex from chicken condensin I revealed by cross-linking and molecular modelling.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4345284{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
