journal = {Science (New York, N.Y.)},
keywords = {Acetylation,Amino Acid Motifs,Benzamides,Benzamides: pharmacology,Cell Line,Cell Nucleus,Cell Nucleus: metabolism,Cell Physiological Phenomena,Cytoplasm,Cytoplasm: metabolism,Enzyme Inhibitors,Enzyme Inhibitors: pharmacology,Histone Deacetylase Inhibitors,Histone Deacetylases,Histone Deacetylases: metabolism,Humans,Hydroxamic Acids,Hydroxamic Acids: pharmacology,Lysine,Lysine: metabolism,Mass Spectrometry,Metabolic Networks and Pathways,Mitochondria,Mitochondria: metabolism,Multiprotein Complexes,Multiprotein Complexes: chemistry,Multiprotein Complexes: metabolism,Post-Translational,Protein Processing,Protein Structure,Proteins,Proteins: chemistry,Proteins: metabolism,Proteome,Proteome: analysis,Proteomics,Pyridines,Pyridines: pharmacology,Saccharomyces cerevisiae,Saccharomyces cerevisiae Proteins,Saccharomyces cerevisiae Proteins: metabolism,Saccharomyces cerevisiae: metabolism,Tertiary,Tumor},
