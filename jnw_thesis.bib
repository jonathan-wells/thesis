pmid = {10022859},
title = {{Correlation between protein and mRNA abundance in yeast.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10022859{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC83965},
volume = {19},
year = {1999}
}
